class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        result = [1] * (len(nums))
        for i in range(1, len(nums)):
            result[i] = result[i - 1] * nums[i - 1]
        right = 1
        length = len(nums)
        for i in range(1, length + 1):
            result[length - i] = right * result[length -i]
            right *= nums[length -i]
        return result
